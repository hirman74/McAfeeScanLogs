        if ( objFSO.FileExists(folReports + fileJSON) ) {
            eachFileRead = objFSO.OpentextFile(folReports + fileJSON, ForReading, dontWantCreateIt , systemDefaultMode); //inside Reports // holder file       
            while(!eachFileRead.AtEndOfStream) {
                var collectJSON = eachFileRead.ReadAll();
            }
