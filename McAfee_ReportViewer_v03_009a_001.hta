    function deleteAssetRow(eachRow) {
        //var folReports = document.getElementById('assetAmmend').innerHTML;
        var folReports = collectLocalFolder () + "Reports\\";
        customFileFolder.checkMakeFolder(folReports);
        var rowSerial = eachRow.toString().split(',')[3];
        //alert(rowSerial + '\r' + folReports);

        var assetfullJSON = loadfullJSON ("assetList_All.json");
        $.each(assetfullJSON.JSONmonth, function(index, dateAdded){
            for (var pcSerial in assetfullJSON.fullJSON[dateAdded]) {
                if (assetfullJSON.fullJSON[dateAdded].hasOwnProperty(pcSerial)) {
                    if (assetfullJSON.fullJSON[dateAdded][pcSerial].SerialNo === rowSerial) delete assetfullJSON.fullJSON[dateAdded][pcSerial];
                }
