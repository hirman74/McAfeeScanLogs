                    <table id="mergeView" class="display compact" data-order='[[ 5, "desc" ]]' data-page-length='10'></table> 
                    <!--<input id="processAllView" type="button" class="buttonGREEN"  value="Rebuild All Records"  onclick="mergedScanAsset() "/>-->
                    Process data from <span class="assetFolder" id='mergeDatafolder'></span>
                    <br>
                    <input  type="button" class="buttonRED"  value="Exit Program"  onclick="closingWindow()"/>
                    <br>
                    <input  type="button" class="buttonGREEN"  value="Refresh Page"  onclick="location.reload()"/>
                    </p>
            </div>
            
            <div id="tabs-2">
                    <p>
                        <h3>Threat Detections for Month of <input style="width: 5%; height:40px; font-size: 16px;border-radius: 4px;"   type="text"  id="dateInputMcAfeeFilter"/>  </h3>
                        <table id="scanReportFilter" class="display compact" data-order='[[ 5, "desc" ]]' data-page-length='10'></table> 
                        <br>                      
                        <input  type="button" class="buttonRED"  value="Exit Program"  onclick="closingWindow()"/>
                        <br>
                        <input  type="button" class="buttonGREEN"  value="Refresh Page"  onclick="location.reload()"/>
                    </p>
            </div>

