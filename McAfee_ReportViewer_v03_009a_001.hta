    function zoomState() {
        var plot3 = $.jqplot('chartBar',[[0]],null);

        if ( document.getElementById('zoomBTN') && document.getElementById('zoomBTN').value && document.getElementById('zoomBTN').childNodes[0] ) {

            var objHolder = loadBarChart ();
            if (objHolder != undefined) {
                var monTicks = objHolder.monTicks,
                pcScanned = objHolder.pcScanned,
                detectThreats = objHolder.detectThreats,
                pcUnknown = objHolder.pcUnknown,
                pcTotalForMonth = objHolder.pcTotalForMonth;

                var barMax = 0, barMin = 0;
                $.each(pcTotalForMonth, function( index, value ) {
                    if (parseInt(value) > barMax) barMax = parseInt(value); //based on total number of PC found, get the maximum PC number for the month.
                });

                if (barMax > 30) {
                    var divideVal = 10;
                } else {
                    var divideVal = 5;
                }

                var group1_2 = {}, sortBigSize = [];
                for (var a=0; a<pcScanned.length; a++) {
                    group1_2 [parseInt(detectThreats[a]) + parseInt(pcUnknown[a])] = parseInt(pcScanned[a]) ; //sum up total for Threats + Unknown as Computed Member Access
                    sortBigSize.push(parseInt(detectThreats[a]) + parseInt(pcUnknown[a])) ;
                }
                sortBigSize.sort(function (a,b){ //small to big
                    return a - b;
                });

                //alert(sortBigSize.join('\r'));
                var group3Value = parseInt(group1_2 [sortBigSize [sortBigSize.length - 1]]);
                //alert (group3Value);
                if (group3Value > 2) {group3Value = 2};

    
                barMin = barMax - parseInt(sortBigSize [sortBigSize.length - 1]) - group3Value ;     
                
                var newBarMax = barMax;
                for (var addMore = 0; addMore < 11; addMore++ ) {
                    if (((barMax + addMore - barMin) % 10) == 0) {
                        newBarMax = barMax + addMore - barMin;
                        break;
                    } else {
                        newBarMax = barMax + addMore - barMin;
                        continue;
                    }
                }   
                
                //alert(newBarMax + '\r' + barMin);       
                if (document.getElementById('zoomBTN').value == "Expand") {
                    plot3.destroy();                    
                    barChartFormat ();

                    document.getElementById('zoomBTN').value = "Focus";
                    document.getElementById('zoomBTN').childNodes[0].nodeValue = "Focus";
                
                } else if (document.getElementById('zoomBTN').value == "Focus")  {

                    barMin = 9999;
                    $.each(pcScanned, function( index, value ) {
                        if (parseInt(value) && (parseInt(value) < barMin)) barMin = parseInt(value);
                    }); 

                    if (barMin > 2) {var newbarMin = barMin - 2} else { var newbarMin = barMin};                    

                    if ( monTicks.length > 0 &&
                    pcScanned.length > 0 &&
                    detectThreats.length > 0 &&
                    pcUnknown.length > 0 ) {

                        plot3.destroy();
                        plot3 = $.jqplot('chartBar', [pcScanned, pcUnknown, detectThreats], {
                            // Tell the plot to stack the bars.
                            seriesColors:[ '#73C774', '#85affd', '#ec8787' ],
                            series: [{label: 'Scanned'},
                                    {label: 'Unverified'}, 
                                    {label: 'Threats Detected'}],
                            stackSeries: true,
                            captureRightClick: false,
                            seriesDefaults:{
                            renderer:$.jqplot.BarRenderer,
                            rendererOptions: {
                                // Put a 30 pixel margin between bars.
                                barMargin: 30,
                                // Highlight bars when mouse button pressed.
                                // Disables default highlighting on mouse over.
                                highlightMouseDown: true,
                                barWidth: 80
                            },
                            pointLabels: {show: false}
                            },
                            axes: {
                                xaxis: {
                                    renderer: $.jqplot.CategoryAxisRenderer,
                                    ticks: monTicks
                                },
                                yaxis: {
                                    // Don't pad out the bottom of the data range.  By default,
                                    // axes scaled as if data extended 10% above and below the
                                    // actual range to prevent data points right on grid boundaries.
                                    // Don't want to do that here.
                                    //padMax: 0
                                    min : newbarMin,
                                    //max: newBarMax,
                                    //numberTicks : (newBarMax - barMin) / 5,
                                    max: barMax,
                                    numberTicks : (barMax - newbarMin) / divideVal,
                                    tickInterval : divideVal
                                }
