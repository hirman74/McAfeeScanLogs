    function clickBarBox (seriesIndex,pointIndex) {
        //var folReports = document.getElementById('mergeDatafolder').innerHTML;
        var folReports = collectLocalFolder () + "Reports\\";
        var foundFiles = folderCollector(folReports,'json', true);      

        var collectedFiles = [];
        foundFiles.forEach(function (nameFile, index){
            if ((nameFile.indexOf("mergeData_") > -1) && (nameFile.indexOf('.json') > -1) && (nameFile.indexOf('mergeData_All.json') == -1) ) {
                var valMonth = nameFile.split('\\').pop().replace('.json','').replace('mergeData_','');
                collectedFiles.push ( [ moment(valMonth,"YYYY-M"),nameFile ] );
            }
        }); 

        /*
        collectedFiles.sort(function(a,b){
            if (a[0] > b[0]) return -1;
            if (a[0] < b[0]) return 1;
        });                
        */

        if (collectedFiles.length > 0) {
            collectedFiles.sort(function(a,b){ //sort from new to old, then remove the older months, only keep latest 6 months
                return -(a[0].diff(b[0])) ;
            });
        }


        var eachFileRead = objFSO.OpentextFile( collectedFiles[pointIndex][1] , ForReading, dontWantCreateIt, systemDefaultMode); 
        while(!eachFileRead.AtEndOfStream) {
            var readAll = eachFileRead.ReadAll();
        }
        var computerArray = JSON.parse(readAll); //load the full file as an object type
        eachFileRead.close(); 

        var dataValues = [];
        for (var key in computerArray) { //the file is a full object literal, can open directly
            if (computerArray.hasOwnProperty(key)) {
                var remarksCol;

                if (computerArray[key].hasOwnProperty('dateString [Endpoint]')) computerArray[key].dateString = computerArray[key]['dateString [Endpoint]'];
                if (computerArray[key].hasOwnProperty('dateString [VirusScan]')) computerArray[key].dateString = computerArray[key]['dateString [VirusScan]'];
                if (computerArray[key].hasOwnProperty('Duration [Endpoint]')) computerArray[key].Duration = computerArray[key]['Duration [Endpoint]'];
                if (computerArray[key].hasOwnProperty('Duration [VirusScan]')) computerArray[key].Duration = computerArray[key]['Duration [VirusScan]'];

                if(seriesIndex == 2) { //Threats Detected
                    if ((computerArray[key].CountDetected !== 0) && (computerArray[key].CountDetected != 'Unknown')) {
                        remarksCol = 'Threats Detected';
                        dataValues.unshift([
                            computerArray[key].Owner,
                            computerArray[key].computerName,
                            computerArray[key].Location,
                            computerArray[key].dateString,
                            computerArray[key].scanType,
                            computerArray[key].Duration,
                            remarksCol                                    
                        ]);                             
                    }
                } else if(seriesIndex == 1) { //Unverified
                    if ( (computerArray[key].CountDetected == 'Unknown')  || (computerArray[key].scanType == "On-Demand Scan") ) {
                        remarksCol = 'Unverified';
                        dataValues.unshift([
                            computerArray[key].Owner,
                            computerArray[key].computerName,
                            computerArray[key].Location,
                            computerArray[key].dateString,
                            computerArray[key].scanType,
                            computerArray[key].Duration,
                            remarksCol                                    
                        ]);                                
                    }                    
                } else if(seriesIndex == 0) { //Scanned
                    if ( (computerArray[key].CountDetected === 0) && (computerArray[key].scanType == "Full Scan") ) {
                        if (computerArray[key].hasOwnProperty('Duration [Endpoint]')) {
                            if (computerArray[key]['Duration [Endpoint]'] != "Unknown") remarksCol = 'Scanned [Endpoint]';
                        } else if (computerArray[key].hasOwnProperty('Duration [VirusScan]')) {
                            if (computerArray[key]['Duration [VirusScan]'] != "Unknown") remarksCol = 'Scanned [VirusScan]';
                        } else if (computerArray[key].hasOwnProperty('Duration')) {
                            if (computerArray[key]['Duration'] != "Unknown") remarksCol = 'Scanned';
                        }  
                        dataValues.unshift([
                            computerArray[key].Owner,
                            computerArray[key].computerName,
                            computerArray[key].Location,
                            computerArray[key].dateString,
                            computerArray[key].scanType,
                            computerArray[key].Duration,
                            remarksCol                                    
                        ]); 
                    }
                }
                    
            }
        }

        $('#mergeView').DataTable().destroy().clear();                            
        var mergeView = $('#mergeView').DataTable({
            data: dataValues,
            columns: [
                { title: "Owner" },
                { title: "HostName" },
                { title: "Location" },
                { title: "Scan Date" },
                { title: "Scan Type" },
                { title: "Scan Duration" },
                { title: "Remarks" }
            ],
            "scrollY": "320px",
            columnDefs: [
                {targets: 0,className: 'dt-body-center'},
                {targets: 1,className: 'dt-body-center'},
                {targets: 2,className: 'dt-body-center'},
                {targets: 3,className: 'dt-body-center'},
                {targets: 4,className: 'dt-body-center'},
                {targets: 5,className: 'dt-body-center'},
                {targets: 6,className: 'dt-body-center'}
            ],
            fixedHeader: true,
            orderClasses: false,
            dom: 'Bfrtip',
            buttons: ['copy']            
        });   

        mergeView.rows().every( function ( rowIdx, tableLoop, rowLoop ) {

            var cell = mergeView.cell({ row: rowIdx, column:6}).node().innerHTML;
            if (cell == 'Threats Detected') {
                cell = mergeView.cell({ row: rowIdx, column:6}).node();
                $(cell).addClass('warning');
                var rowEle = mergeView.row(rowIdx).node();
                $(rowEle).addClass('warning');
            }

            var cell = mergeView.cell({ row: rowIdx, column:6}).node().innerHTML;
            if (cell == 'Unverified') {
                cell = mergeView.cell({ row: rowIdx, column:6}).node();
                $(cell).addClass('unverified');
                var rowEle = mergeView.row(rowIdx).node();
                $(rowEle).addClass('unverified');                                    
            }                     


            var cell = mergeView.cell({ row: rowIdx, column:6}).node().innerHTML;
            if ((cell == 'Scanned') || (cell == 'Scanned [Endpoint]') || (cell == 'Scanned [VirusScan]')) {
                cell = mergeView.cell({ row: rowIdx, column:6}).node();
                $(cell).addClass('scanGood');
                var rowEle = mergeView.row(rowIdx).node();
                $(rowEle).addClass('scanGood');                                        
            } 

        });   
        $($.fn.dataTable.tables(true)).DataTable().columns.adjust(); 

    }

