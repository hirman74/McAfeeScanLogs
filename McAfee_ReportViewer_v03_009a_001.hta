    function folderCollector(folderGet, checkExt, sortingASC){
        var fileLists = []; 
        var folderGet = objFSO.GetFolder(folderGet);
        var eachFiles = new Enumerator(folderGet.files);        
        for (; !eachFiles.atEnd(); eachFiles.moveNext()){
            var splitDot = eachFiles.item().path.split('.');
            var checkExtensions = splitDot[splitDot.length-1];
            if (checkExtensions == checkExt){
                fileLists.push (eachFiles.item().path); //full filename path
                if (!sortingASC) {
                    fileLists.reverse();
                }
            }
        } 
        return fileLists;
    }

    function collectLocalFolder () {

        var linkCollect = document.getElementById('localREF').href;
		
		if (linkCollect.indexOf('file\:\/\/\/') > -1) {
			linkCollect = linkCollect.split('file\:\/\/\/')[1]; //both local/network drives will have file:///
		} else if (linkCollect.indexOf('file\:\/\/') > -1) {linkCollect = linkCollect.split('file\:\/\/')[1];} //URL drives will have file://
       
        if ((linkCollect.split('')[0].match(/^[A-Za-z]+$/) != null) && (linkCollect.split('')[1].match(/\:/) != null))  { //drive letter with colon :
            linkCollect = linkCollect.replace(/\//g,'\\');
        } else {
            linkCollect = linkCollect.replace(/\//g,'\\');
			linkCollect = linkCollect.replace(/\%20/g,' ');
            linkCollect = '\\\\' + linkCollect; //ip address prepend with \\
        }

        var folConsolidated = linkCollect + '\\Consolidated\\';
        var folReports = linkCollect + '\\Reports\\';

        customFileFolder.checkMakeFolder(folReports);
        customFileFolder.checkMakeFolder(folConsolidated); 


        return linkCollect;
        //return document.getElementById('localREF').href.split('file:\/\/\/')[1].replace(/\//g,'\\');
    }

