    function IsJsonString(str) {
        try {
            JSON.parse(str);
        } catch (e) {
            return false;
        }
        return true;
    }

    function collectConsolidated() {
        var folConsolidated = collectLocalFolder () + "Consolidated\\";
        var keepOldLogs = collectLocalFolder () + "Consolidated\\KeepLogs\\";
        var folReports = collectLocalFolder () + "Reports\\";

        customFileFolder.checkMakeFolder(folConsolidated);   
        customFileFolder.checkMakeFolder(keepOldLogs);   
        customFileFolder.checkMakeFolder(folReports);

        var fileDateStamp = [];
        var fileObj = {};          

        //check the file exists and read the file 50 times to make sure the content does not change before proceeding
        if ( objFSO.FileExists(folReports + "scanLog_All.json") ) {
            var fileGet = objFSO.GetFile(folReports + "scanLog_All.json");  

            eachFileRead = objFSO.OpentextFile(folReports + "scanLog_All.json", ForReading, dontWantCreateIt , systemDefaultMode); //inside Reports // holder file       
            while(!eachFileRead.AtEndOfStream) {
                var readJSON = eachFileRead.ReadAll();
            }
            eachFileRead.close();

            fileGet = objFSO.GetFile(folReports + "scanLog_All.json"); //first time collect data from scan info
            fileObj.accessed = fileGet.DateLastAccessed;
            fileObj.modified = fileGet.DateLastModified;
            fileDateStamp[0] = fileObj;

            if (IsJsonString(readJSON)) {
                var scanAllJSON = JSON.parse(readJSON); 
            } else {
                var scanAllJSON = {};
            }
        } else if ( !objFSO.FileExists(folReports + "scanLog_All.json") ) {
            var scanAllJSON = {};
            eachFileRead = objFSO.OpentextFile(folReports + "scanLog_All.json", ForWriting , CreateIt , systemDefaultMode); //inside Reports // holder file       
            eachFileRead.close();

            fileGet = objFSO.GetFile(folReports + "scanLog_All.json"); //first time collect data from scan info
            fileObj.accessed = fileGet.DateLastAccessed;
            fileObj.modified = fileGet.DateLastModified;
            fileDateStamp[0] = fileObj;
        }

        var folderGet = objFSO.GetFolder(folConsolidated);
        var fileLists = folderCollector(folderGet,"log", false); //sort in reverse to get the latest file first

        if (fileLists.length) {} else {
            return; //no files to process
        }

        var toKeepFiles = []; //delete files if imported file correctly
        for (var a = 0; a < fileLists.length; a++) {

            if ( fileLists[a].match(/\_ver02\_/g) != null ) {
                eachFileRead = objFSO.OpentextFile(fileLists[a], ForReading, dontWantCreateIt , systemDefaultMode); //inside Reports // holder file       
                while(!eachFileRead.AtEndOfStream) {
                    var readJSON = eachFileRead.ReadAll();
                }
                eachFileRead.close();                           

                if (IsJsonString(readJSON)) { //testing if each log files collected is a correct JSON formatted content
                    toKeepFiles.push(fileLists[a]); //file can be deleted once is imported
                    var perFileJSON = JSON.parse(readJSON); 
                    for (var dateKey in perFileJSON) { //format "YYYY-M" 
                        if (perFileJSON.hasOwnProperty(dateKey)) {
                            var foundAllDateKey = false; //using this to check if scanLog_All.json has this date
                            var foundAllHostKey = false; //using this to check if scanLog_All.json has this host PC in this date

                            for (var key in scanAllJSON) { //format "YYYY-M"
                                if (scanAllJSON.hasOwnProperty(key) && (key == dateKey)) { //checking if found the date found in each host log file to the centralised records
                                    foundAllDateKey = true;
                                    for (var hostKey in scanAllJSON[key]) {
                                        if (scanAllJSON[key].hasOwnProperty(hostKey) && (hostKey == perFileJSON[dateKey].computerName) && (key == dateKey)) { 
                                            foundAllHostKey = true;
                                        }
                                    }
                                    if (!foundAllHostKey){
                                        scanAllJSON[key][perFileJSON[dateKey].computerName] = perFileJSON[dateKey]; //each mon year, should have object key of PC hostname 
                                        foundAllHostKey = true;
                                    }
                                }
                            }
                            if (!foundAllDateKey) { //meaning this full centralised record, do not even have this date group which is found in the per host log file
                                scanAllJSON[dateKey] = {};
                                scanAllJSON[dateKey][perFileJSON[dateKey].computerName] = perFileJSON[dateKey];
                                foundAllDateKey = true;
                            }
                        }
                    }
                } 
            } else if ( fileLists[a].match(/\_ver01\_/g) != null ) { //delete old version files if leftover
                customFileFolder.deleteFile(fileLists[a]);
            } else if  ( fileLists[a].match(/\_ver03\_/g) != null ) { //data can be VirusScan or Endpoint records

                eachFileRead = objFSO.OpentextFile(fileLists[a], ForReading, dontWantCreateIt , systemDefaultMode); //inside Reports // holder file       
                while(!eachFileRead.AtEndOfStream) {
                    var readJSON = eachFileRead.ReadAll();
                }
                eachFileRead.close();  

                //20191126 1455
                if (IsJsonString(readJSON)) { //testing if each log files collected is a correct JSON formatted content
                    toKeepFiles.push(fileLists[a]); //file can be moved once is imported
                    var perFileJSON = JSON.parse(readJSON); 

                    for (var hostDate in perFileJSON) { //format "YYYY-M" This is date for the collected PC log JSON file

                        if (perFileJSON.hasOwnProperty(hostDate)) {
                            var foundAllDateKey = false; //using this to check if scanLog_All.json has this date
                            var foundAllHostKey = false; //using this to check if scanLog_All.json has this host PC in this date

                            for (var scanDate in scanAllJSON) { //format "YYYY-M" this is the full record file. for each date month loop
                                if (scanAllJSON.hasOwnProperty(scanDate) && (scanDate == hostDate)) { //checking if found the date found in each host log file to the centralised records
                                    foundAllDateKey = true;
                                    for (var hostKey in scanAllJSON[scanDate]) {
                                        if (scanAllJSON[scanDate].hasOwnProperty(hostKey) && (hostKey == perFileJSON[hostDate].computerName) && (scanDate == hostDate)) { 
                                            for (eachElem in perFileJSON[hostDate]) {
                                                if (perFileJSON[hostDate].hasOwnProperty(eachElem)) {
                                                    scanAllJSON[scanDate][perFileJSON[hostDate].computerName][eachElem] = perFileJSON[hostDate][eachElem]; //transfering host info into the central scanAllJSON data \\seperation by eachElem ensure that any existing same name entry not deleted
                                                }
                                            }
                                            foundAllHostKey = true;
                                        }
                                    }
                                    if (!foundAllHostKey){
                                        scanAllJSON[scanDate][perFileJSON[hostDate].computerName] = perFileJSON[hostDate]; //importing the whole host log file into the central record
                                        foundAllHostKey = true;
                                    }
                                }
                            }
                            if (!foundAllDateKey) { //meaning this full centralised record, do not even have this date group which is found in the per host log file
                                scanAllJSON[hostDate] = {};
                                scanAllJSON[hostDate][perFileJSON[hostDate].computerName] = perFileJSON[hostDate]; //placing the whole found host object into the records
                                foundAllDateKey = true;
                            }
                        }
                    }
                }                                   
            }
        }


        //make sure that original file has not changed after adding data from each PC log files
        if ( objFSO.FileExists(folReports + "scanLog_All.json") ) {
            fileGet = objFSO.GetFile(folReports + "scanLog_All.json"); //before writing back, make sure no one had modified before
            fileObj.accessed = fileGet.DateLastAccessed;
            fileObj.modified = fileGet.DateLastModified;
            fileDateStamp[1] = fileObj; //collect the file date time again to confirm no changes already made        
        } 

        if ( fileDateStamp[0].modified == fileDateStamp[1].modified ) { //over writing the same file with collected data, if file date change, will not write file. Since someone else has started process it
            //temporarily write with full data. but later will over write with 6 months data only
            var eachJSONRead = objFSO.OpentextFile( folReports + "scanLog_All.json" , ForWriting, CreateIt , systemDefaultMode); //any found new added stuff, write over the same file
            eachJSONRead.write(JSON.stringify(scanAllJSON));
            eachJSONRead.close();

            collectHostMcAfee (); //this is to collect all the available Host Name found from the processed consolidated folder which stores the scan records as scanLog_All.json

            var JSONsize = 0, JSONmonth = [];
            for (var key in scanAllJSON) { //purpose of checking latest McAfee Data, and only keep last 6 latest month records
                if (scanAllJSON.hasOwnProperty(key)) {
                    JSONsize++;
                    JSONmonth.push(key)
                }
            }  

            if (JSONsize > 0) {
                JSONmonth.sort(function(a,b){ //sort from new to old, then remove the older months, only keep latest 6 months
                    return -(moment(a,'YYYY-M').diff(moment(b,'YYYY-M')));
                });
            } 

            var foundFiles = folderCollector(folReports,'json', true); //this function collect all the json file into an array
            for (var a=0; a<foundFiles.length; a++){
                if ((foundFiles[a].indexOf("scanLog_") > -1) && (foundFiles[a].indexOf('.json') > -1) && (foundFiles[a].indexOf('scanLog_All.json') == -1) ) {
                    customFileFolder.deleteFile(foundFiles[a]); //delete all the month scan report before proceeding to create new latest 6 months records
                }
            }        

            var count=0;
            for (var a=0; a<JSONmonth.length; a++) {
                if (scanAllJSON.hasOwnProperty(JSONmonth[a]) && (count < 6)){ //only write file for latest 6 months
                    var eachJSONRead = objFSO.OpentextFile( folReports + "scanLog_" + JSONmonth[a] + '.json', ForWriting, CreateIt , systemDefaultMode); //centralised log should be keeping super old history logs
                    eachJSONRead.write(JSON.stringify(scanAllJSON[JSONmonth[a]]));
                    eachJSONRead.close();
                    count++;
                }            
            }

            if (JSONsize > 0) {
                JSONmonth.sort(function(a,b){ //sort from old to new, then remove the older months, only keep latest 6 months
                    return moment(a,'YYYY-M').diff(moment(b,'YYYY-M'));
                });
            }         

            var toDelete = JSONmonth.length - 6;
            count = 0;
            for (var a=0; a<JSONmonth.length; a++) {
                if (scanAllJSON.hasOwnProperty(JSONmonth[a]) && (count < toDelete)){ //only keep latest 6 months
                    delete scanAllJSON[JSONmonth[a]];
                    count++;
                }            
            }

            var eachJSONRead = objFSO.OpentextFile( folReports + "scanLog_All.json" , ForWriting, CreateIt , systemDefaultMode); //any found new added stuffs in written over the same file
            eachJSONRead.write(JSON.stringify(scanAllJSON));
            eachJSONRead.close();

            toKeepFiles.forEach(function (nameFile, index){ 
                var eachFileName = nameFile.split('\\').pop();
                customFileFolder.copyFile(nameFile, keepOldLogs + eachFileName, true );
                customFileFolder.deleteFile(nameFile);
            });
            
            matchHostname(false);     
        }  

        alert("McAfee log files consolidated!");

        
    }

    function convertInteger (value, default_value, radix) {
        try{
            radix = radix || 10;
            default_value = default_value || 0;
            if ( value.indexOf('Unknown') == -1 || value.indexOf('N.A.') == -1 ) var that = parseInt (value, radix);
            return that
        } catch (err) {}
        return default_value
    }    

    function fillNA (perOBJ) {

        perOBJ["Cookies detected"] = 'N.A.'; //only VirusScan have extra, newly added for version 3
        perOBJ["Registry detections"]  = 'N.A.'; //only Endpoint have extra, newly added for version 3
        
        if (perOBJ.hasOwnProperty('dateString [Endpoint]')) {
            perOBJ.dateString = perOBJ['dateString [Endpoint]']; 
        } else if (perOBJ.hasOwnProperty('dateString [VirusScan]')) {
            perOBJ.dateString = perOBJ['dateString [VirusScan]']; 
        }

        if (perOBJ.hasOwnProperty('Duration [Endpoint]')) {
            perOBJ.Duration = perOBJ['Duration [Endpoint]']; 
        } else if (perOBJ.hasOwnProperty('Duration [VirusScan]')) {
            perOBJ.Duration = perOBJ['Duration [VirusScan]']; 
        }

        if (perOBJ.hasOwnProperty("Processes detected [VirusScan]")) perOBJ["Processes detected"] =  perOBJ["Processes detected [VirusScan]"];
        if (perOBJ.hasOwnProperty("Boot sectors detected [VirusScan]")) perOBJ["Boot sectors detected"] = perOBJ["Boot sectors detected [VirusScan]"];
        if (perOBJ.hasOwnProperty("Files with detections [VirusScan]")) perOBJ["Files with detections"] = perOBJ["Files with detections [VirusScan]"];
        if (perOBJ.hasOwnProperty("File detections [VirusScan]")) perOBJ["File detections"] = perOBJ["File detections [VirusScan]"];
        if (perOBJ.hasOwnProperty("Keys detected [VirusScan]")) perOBJ["Keys detected"] = perOBJ["Keys detected [VirusScan]"];
        if (perOBJ.hasOwnProperty("Cookies detected [VirusScan]")) perOBJ["Cookies detected"] = perOBJ["Cookies detected [VirusScan]"];
        if (perOBJ.hasOwnProperty("Files with detections [Endpoint]")) {
            perOBJ["Processes detected"] = 'N.A.',
            perOBJ["Boot sectors detected"] = 'N.A.',
            perOBJ["Files with detections"] = perOBJ["Files with detections [Endpoint]"],
            perOBJ["File detections"] = 'N.A.',
            perOBJ["Keys detected"] = 'N.A.',
            perOBJ["Cookies detected"] = 'N.A.'; 
        }

        if (perOBJ.hasOwnProperty("Registry detections [Endpoint]")) perOBJ["Registry detections"]  = perOBJ["Registry detections [Endpoint]"];
        return perOBJ
    }

    function calcTotalVirus (perOBJ){
        totalVirus = 0;
        totalVirus += convertInteger (perOBJ["Processes detected"], 0, 10);
        totalVirus += convertInteger (perOBJ["Boot sectors detected"], 0, 10);
        totalVirus += convertInteger (perOBJ["Files with detections"], 0, 10);
        totalVirus += convertInteger (perOBJ["File detections"], 0, 10);
        totalVirus += convertInteger (perOBJ["Keys detected"], 0, 10);


        //for version 03
        totalVirus += convertInteger (perOBJ["Files with detections [Endpoint]"], 0, 10);
        totalVirus += convertInteger (perOBJ["Registry detections [Endpoint]"], 0, 10);
        totalVirus += convertInteger (perOBJ["Processes detected [VirusScan]"], 0, 10);
        totalVirus += convertInteger (perOBJ["Boot sectors detected [VirusScan]"], 0, 10);
        totalVirus += convertInteger (perOBJ["Files with detections [VirusScan]"], 0, 10);
        totalVirus += convertInteger (perOBJ["File detections [VirusScan]"], 0, 10);
        totalVirus += convertInteger (perOBJ["Keys detected [VirusScan]"], 0, 10);
        totalVirus += convertInteger (perOBJ["Cookies detected [VirusScan]"], 0, 10);        
        return totalVirus
    }

