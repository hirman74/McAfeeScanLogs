    function loadBarChart () {
        var monTicks = [];
        var pcUnknown = [];
        var detectThreats = [];
        var pcScanned = [];
        var pcTotalForMonth = [];
        var nowYYYYmm =  moment().year() + '_' + (parseInt(moment().month()) + 1) ;

        /*
        var folConsolidated = document.getElementById('collectFrom').innerHTML.split('\\');
        folConsolidated.pop();
        folConsolidated.pop();
        var folReports = folConsolidated.join('\\') + '\\Reports\\';
        */

        //var folReports = document.getElementById('mergeDatafolder').innerHTML;
        var folReports = collectLocalFolder () + "Reports\\";

            var foundFiles = folderCollector(folReports,'json', true); //this function collect all the json file into an array
            var collectedFiles = []; 

            for (var a=0; a<foundFiles.length; a++){
                if ((foundFiles[a].indexOf("mergeData_") > -1) && (foundFiles[a].indexOf('.json') > -1) && (foundFiles[a].indexOf('mergeData_All.json') == -1) ) {
                    var valMonth = foundFiles[a].split('\\').pop().replace('.json','').replace('mergeData_','');
                    collectedFiles.push ( [ moment(valMonth,"YYYY-M"),foundFiles[a] ] );
                }
            }

            if (collectedFiles.length > 5) {
                collectedFiles.sort(function(a,b){
                    if (a[0] > b[0]) return -1;
                    if (a[0] < b[0]) return 1;
                });

                for (var a=0; a<6; a++) {
                    var countUnknown = 0;
                    var countDetected = 0;
                    var countScanned = 0;

                    monTicks.push(moment(collectedFiles[a][0]).format('MMM YYYY')); //collected month value     
                    
                    var eachFileRead = objFSO.OpentextFile( collectedFiles[a][1], ForReading, dontWantCreateIt, systemDefaultMode); 
                    while(!eachFileRead.AtEndOfStream) {
                        var readAll = eachFileRead.ReadAll();
                    }
                    eachFileRead.close();

                    if (IsJsonString(readAll)) {
                        var computerArray = JSON.parse(readAll); //load the full file as an object type
                        for (var key in computerArray) { //the file is a full object literal, can open directly
                            if (computerArray.hasOwnProperty(key)) {

                                if ( computerArray[key].CountDetected == "Unknown" || computerArray[key].scanType == "On-Demand Scan" ) {countUnknown++;
                                    } else if (computerArray[key].CountDetected === 0  && computerArray[key].scanType == "Full Scan" ) {countScanned++;
                                    } else if (computerArray[key].CountDetected > 0) {countDetected++;
                                }
                            }
                        } 
                    }
  
                    pcUnknown.push(countUnknown); //add PC unverified for the month
                    pcScanned.push(countScanned); //total green count for the month
                    detectThreats.push(countDetected); //total red count for the month
                    pcTotalForMonth.push(countUnknown + countScanned + countDetected);
                }                

                return {
                    monTicks: monTicks,
                    pcUnknown: pcUnknown,
                    detectThreats: detectThreats,
                    pcScanned: pcScanned,
                    pcTotalForMonth: pcTotalForMonth
                }
            }
    }

