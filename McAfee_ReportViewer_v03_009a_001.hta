                                        var collectJSON = eachFileRead.ReadAll();
                                    }
                                    eachFileRead.close();
                                    var fullJSON = JSON.parse(collectJSON); 
                                }

                                var countKey = 0;
                                for (var key in fullJSON) {
                                    if (fullJSON.hasOwnProperty(key)) {
                                        countKey++;
                                    }
                                }

                                if (!countKey) {
                                    fullJSON[newYYYYmm] = {};
                                    fullJSON[newYYYYmm][newHostName] = {}
                                    fullJSON[newYYYYmm][newHostName].scanLog = [];
                                    var inCont = {};
                                    inCont.DateTime = newAddDate;
                                    inCont.Detected = newDetected;  
                                    fullJSON[newYYYYmm][newHostName].scanLog.push(inCont);
                                }

                                var foundYearKey = false;
                                var foundHostKey = false;
                                for (var a in fullJSON) {
                                    if (fullJSON.hasOwnProperty(a) && (a == newYYYYmm)) {
                                        foundYearKey = true;
                                        for (var b in fullJSON[a]) { //yyyy_mm
                                            if (fullJSON[a].hasOwnProperty(b) && (b == newHostName)) {
                                                foundHostKey = true;
                                                if (fullJSON[a][newHostName].hasOwnProperty(scanLog)) {
                                                    var inObj = {};
                                                    inObj.DateTime = newAddDate;
                                                    inObj.Detected = newDetected;
                                                    fullJSON[a][b].scanLog.push(inObj); 
                                                } else { 
                                                    fullJSON[a][b] = {};
                                                    fullJSON[a][b].scanLog = [];
                                                    var inObj = {};
                                                    inObj.DateTime = newAddDate;
                                                    inObj.Detected = newDetected;
                                                    fullJSON[a][b].scanLog.push(inObj);
                                                }
                                            }
                                        } 
                                    }                                    
                                }

                                if (!foundYearKey) {
                                    fullJSON[newYYYYmm] = {};
                                    fullJSON[newYYYYmm][newHostName] = {}
                                    fullJSON[newYYYYmm][newHostName].scanLog = [];
                                    var inCont = {};
                                    inCont.DateTime = newAddDate;
                                    inCont.Detected = newDetected;  
                                    fullJSON[newYYYYmm][newHostName].scanLog.push(inCont);
                                    foundYearKey = true;
                                } else if (!foundHostKey) {
                                    fullJSON[newYYYYmm][newHostName] = {}
                                    fullJSON[newYYYYmm][newHostName].scanLog = [];
                                    var inCont = {};
                                    inCont.DateTime = newAddDate;
                                    inCont.Detected = newDetected;  
                                    fullJSON[newYYYYmm][newHostName].scanLog.push(inCont);
                                    foundHostKey = true;
                                }

                            } else {
                                if (confirm ("Please check the virus detect count '" + eachAddScan[2] + "' is a number format.\nFor PC : " + eachAddScan[0] + "\n\nWill you like to try adding rests of the records? \n\nClick OK to continue adding next record.\nClink Cancel to stop adding anymore records." )) {
                                    continue;
                                } else {
                                    return;
                                }                                
                            }

                        } else {
