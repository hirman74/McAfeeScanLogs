        }  

        if (JSONmonth.length){
            JSONmonth.sort(function(a,b){ //presumably sort from old to new
                return moment(a,'YYYY-M').diff(moment(b,'YYYY-M'));
            });
        } 

        return {
            fullJSON:fullJSON, //object
            JSONsize:JSONsize, //length
            JSONmonth:JSONmonth //array
        }

    }

    function makeUnique(a) {
        var seen = {};
        var out = [];
        var len = a.length;
        var j = 0;
        for(var i = 0; i < len; i++) {
            var item = a[i];
            if(seen[item] !== 1) {
                seen[item] = 1;
                out[j++] = item;
            }
        }
        return out;
    }

